{
  "name": "@inrupt/lit-artifact-generator",
  "version": "0.13.4",
  "description": "A generator for building artifacts (e.g. Java JARs, NPM modules, etc.) from RDF vocabularies.",
  "main": "index.js",
  "bin": {
    "lit-artifact-generator": "index.js"
  },
  "scripts": {
    "test": "npm run format && npm run lint && jest",
    "start": "node index.js",
    "format": "prettier --write \"index.js\" \"src/**/*.js\"",
    "lint": "eslint src --ext js,jsx",
    "preversion": "npm test"
  },
  "author": "Inrupt, Inc.",
  "contributors": [
    "Jarlath Holleran (https://github.com/jholleran)",
    "Pat McBennett (https://github.com/pmcb55)",
    "Nicolas Seydoux (https://github.com/NSeydoux)"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:inrupt/lit-artifact-generator.git"
  },
  "license": "MIT",
  "preferGlobal": true,
  "keywords": [],
  "dependencies": {
    "@rdfjs/fetch-lite": "^2.1.1",
    "@rdfjs/formats-common": "^2.1.0",
    "@rdfjs/parser-jsonld": "^1.2.1",
    "@rdfjs/parser-n3": "^1.1.4",
    "@rdfjs/serializer-ntriples": "^1.0.3",
    "@rdfjs/sink-map": "^1.0.1",
    "axios": "^0.21.1",
    "chokidar": "3.5.1",
    "debug": "^4.3.2",
    "del": "^6.0.0",
    "glob": "^7.1.7",
    "handlebars": "^4.7.7",
    "inquirer": "^7.3.3",
    "js-yaml": "^3.14.1",
    "mock-local-storage": "^1.1.17",
    "moment": "^2.29.1",
    "rdf-ext": "^1.3.1",
    "rdfa-streaming-parser": "^1.4.0",
    "rdfxml-streaming-parser": "^1.4.0",
    "rimraf": "^3.0.2",
    "string-to-stream": "^3.0.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/generated/",
      "<rootDir>/test/Generated/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "functions": 100,
        "statements": 100,
        "branches": 100
      }
    }
  }
}
