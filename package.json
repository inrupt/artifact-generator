{
  "name": "@inrupt/lit-artifact-generator",
  "version": "0.7.0",
  "description": "A generator for building artifacts (e.g. Java JARs, NPM modules, etc.) from RDF vocabularies.",
  "main": "index.js",
  "bin": {
    "lit-artifact-generator": "./index.js"
  },
  "scripts": {
    "test": "npm run format && npm run lint && jest",
    "start": "node index.js",
    "format": "prettier --write \"index.js\" \"src/**/*.js\"",
    "lint": "eslint src --ext js,jsx",
    "preversion": "npm test"
  },
  "author": "Inrupt, Inc.",
  "contributors": [
    "Jarlath Holleran (https://github.com/jholleran)",
    "Pat McBennett (https://github.com/pmcb55)",
    "Nicolas Seydoux (https://github.com/NSeydoux)"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:inrupt/lit-artifact-generator-js.git"
  },
  "license": "MIT",
  "preferGlobal": true,
  "keywords": [],
  "dependencies": {
    "axios": "^0.19.0",
    "@rdfjs/formats-common": "^2.0.1",
    "@rdfjs/fetch-lite": "^2.0.1",
    "@rdfjs/parser-jsonld": "^1.1.2",
    "@rdfjs/parser-n3": "^1.1.3",
    "@rdfjs/sink-map": "^1.0.1",
    "debug": "^4.1.1",
    "del": "^4.1.1",
    "handlebars": "^4.1.2",
    "inquirer": "^6.4.1",
    "js-yaml": "^3.13.1",
    "mock-local-storage": "^1.1.8",
    "moment": "^2.24.0",
    "rdf-ext": "^1.3.0",
    "rdfxml-streaming-parser": "^1.3.1",
    "string-to-stream": "^3.0.1",
    "yargs": "^13.2.4",
    "chokidar": "3.2.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^25.4.0",
    "prettier": "^1.18.2"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/generated/",
      "<rootDir>/test/Generated/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "functions": 100,
        "statements": 100,
        "branches": 100
      }
    }
  }
}
