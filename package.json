{
  "name": "@lit/artifact-generator",
  "version": "0.1.0",
  "description": "A generator for building NPM modules (artifacts) from RDF vocabularies",
  "main": "index.js",
  "bin": {
    "lit-artifact-generator": "./index.js"
  },
  "scripts": {
    "test": "npm run format && npm run lint && jest",
    "start": "node index.js",
    "format": "prettier --single-quote --trailing-comma es5 --write \"index.js\" \"src/**/*.js\"",
    "lint": "eslint src -c .eslintrc.json --ext js,jsx"
  },
  "author": "inrupt, Inc.",
  "contributors": [
    "Jarlath Holleran (https://github.com/jholleran)",
    "Pat McBennett (https://github.com/pmcb55)"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:inrupt/lit-artifact-generator-js.git"
  },
  "license": "MIT",
  "preferGlobal": true,
  "keywords": [],
  "dependencies": {
    "@lit/generated-vocab-common": "^0.1.0",
    "@lit/vocab-term": "^0.1.0",
    "@rdfjs/fetch-lite": "^2.0.1",
    "@rdfjs/parser-jsonld": "^1.1.2",
    "@rdfjs/parser-n3": "^1.1.3",
    "@rdfjs/sink-map": "^1.0.1",
    "debug": "^4.1.1",
    "del": "^4.1.1",
    "handlebars": "^4.1.2",
    "inquirer": "^6.4.1",
    "js-yaml": "^3.13.1",
    "mock-local-storage": "^1.1.8",
    "moment": "^2.24.0",
    "rdf-ext": "^1.3.0",
    "rdfxml-streaming-parser": "^1.3.1",
    "yargs": "^13.2.4",
    "chokidar": "3.2.2"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "prettier": "^1.18.2"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/generated/",
      "<rootDir>/test/generated/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "functions": 100,
        "statements": 100,
        "branches": 100
      }
    }
  }
}
