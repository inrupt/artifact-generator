{
  "name": "{{npmModuleScope}}{{artifactName}}",
  "version": "{{artifactVersion}}",
  "description": "{{description}}",
  "main": "index.js",
{{#if supportBundling}}
  "scripts": {
    "dev": "webpack --config ./config/webpack.dev.config.js",
    "prod": "webpack --config ./config/webpack.prod.config.js"
  },
{{/if}}
  "author": "{{authors}}",
  "license": "MIT",
{{#if gitRepository}}
  "repository": {
    "type": "git",
    "url": "{{gitRepository}}"
  },
{{/if}}
{{#if repository}}
  {{#each repository}}
    {{#if enabled}}
  "publishConfig": {
    "registry": "{{registry}}"
  },
    {{/if}}
 {{/each}}
{{/if}}
  "dependencies": {
    "@lit/vocab-term": "{{litVocabTermVersion}}"
  }{{#if supportBundling}},
  "devDependencies": {
    "@babel/core": "{{versionBabelCore}}",
    "babel-loader": "{{versionBabelLoader}}",
    "webpack": "{{versionWebpack}}",
    "webpack-cli": "{{versionWebpackCli}}"
  }{{/if}}
}
