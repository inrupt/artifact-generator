const namespace as _namespace = require("@rdfjs/namespace");
const rdfFactory as _rdfFactory = require('@rdfjs/data-model')

const _RDFJS_NAMESPACE = _namespace("{{namespace}}");

const _NAMESPACE = "{{namespace}}";
function _NS (localName) {
