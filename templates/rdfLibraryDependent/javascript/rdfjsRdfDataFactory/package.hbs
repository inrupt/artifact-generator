{
  "name": "{{npmModuleScope}}{{artifactPrefix}}{{artifactName}}{{artifactSuffix}}",
  "version": "{{artifactVersion}}",
  "description": "{{{description}}}",
  "sideEffects": false,
{{#if supportBundling}}
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "scripts": {
    "build": "rollup --config config/rollup.config.js"
  },
{{else}}
  "main": "index.js",
  "module": "wrapper.js",
{{/if}}
  "author": {
    "name": "{{generatorName}}",
    "url": "https://github.com/inrupt/lit-artifact-generator"
  },
{{#if contributors}}
  "contributors": [
    {{#each contributors}}
    {"name": "{{this}}"}{{#unless @last}},{{/unless}}
    {{/each}}
  ],
{{/if}}
{{#if license}}
  "license": "{{license/name}}",{{/if}}{{#if versioning}}{{#if versioning.url}}
  "repository": {
    "type": "{{versioning.type}}",
    "url": "{{versioning.url}}"
  },{{/if}}{{/if}}
  "dependencies": {
    "rdf-data-factory": "{{rdfjsImplVersion}}"
  }{{#if supportBundling}},
  "devDependencies": {
    "@babel/core": "{{babelCoreVersion}}",
    "rollup": "{{rollupVersion}}",
    "@rollup/plugin-babel": "{{rollupBabelPluginVersion}}",
    "@rollup/plugin-commonjs": "{{rollupCommonjsPluginVersion}}",
    "@rollup/plugin-node-resolve": "{{rollupNodeResolveVersion}}"
  }{{/if}}
}
