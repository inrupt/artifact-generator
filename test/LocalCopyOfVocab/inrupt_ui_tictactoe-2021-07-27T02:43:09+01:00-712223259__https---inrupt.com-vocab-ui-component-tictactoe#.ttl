    vann:preferredNamespaceUri "https://inrupt.com/vocab/ui/component/tictactoe#".
inrupt_ui_tictactoe:createGameSuccess a rdfs:Literal;
    skos:definition "Game created successfully"@en, "Juego creado satisfactoriamente"@es.
inrupt_ui_tictactoe:invitationTemplate a rdfs:Literal;
    skos:definition "<0><0><0>{{- name}}</0> has invited you to play a game of TicTacToe.</0><1>Would you like to play?</1></0>"@en, "<0><0><0>{{- name}}</0> te ha invitado a un juego de Tres en Linea.</0><1>Quieres aceptar el juego?</1></0>"@es.
inrupt_ui_tictactoe:msgTicTacToeMove a rdfs:Literal;
    skos:definition "Tictactoe move"@en, "Movimiento Tictactoe"@es.
inrupt_ui_tictactoe:msgMoveMade a rdfs:Literal;
    skos:definition "A move has been made in your Tic-Tac-Toe game."@en, "Se ha realizado un movimiento en tu juego Tic-Tac-Toe."@es.
inrupt_ui_tictactoe:opponentWebId a rdfs:Literal;
    skos:definition "Opponent WebID"@en, "WebID del oponente"@es;
    rdfs:comment "Enter the WebID of your opponent - when you click 'Create Game' they will receive a notification that you wish to play a game of Tic-Tac-Toe with them."@en, "Ingrese el WebID de su oponente: cuando haga clic en 'Crear juego', recibirá una notificación de que desea jugar un juego de Tic-Tac-Toe con ellos."@es;
    rdfs:seeAlso <https://team.inrupt.net/public/lit/seeAlso-opponentWebId.html>.
