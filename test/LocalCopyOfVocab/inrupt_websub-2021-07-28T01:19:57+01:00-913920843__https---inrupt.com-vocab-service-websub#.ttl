@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix vann: <http://purl.org/vocab/vann/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix schema: <http://schema.org/>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix time: <http://www.w3.org/2006/time#>.
@prefix inrupt_websub: <https://inrupt.com/vocab/service/websub#>.

<https://inrupt.com/vocab/service/websub#> a owl:Ontology, <https://inrupt.com/vocab/tool/artifact_generator#Ontology>;
    owl:versionInfo "0.1";
    dcterms:title "Inrupt WebSub vocabulary"@en;
    dcterms:description "The Inrupt WebSub vocabulary."@en;
    dcterms:creator <https://inrupt.com/profile/card/#us>;
    dcterms:issued "2019/11/08"^^xsd:date;
    dcterms:license "http://purl.org/NET/rdflicense/MIT1.0.ttl";
    vann:preferredNamespacePrefix "inrupt_websub";
    vann:preferredNamespaceUri "https://inrupt.com/vocab/service/websub#".
inrupt_websub:hub a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Hub"@en;
    rdfs:comment "Hub to which a publisher publishes resource updates."@en.
inrupt_websub:self a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Self"@en;
    rdfs:comment "Refers to ourselves as a resource (e.g. can be used as the value of a Link\n header)."@en.
inrupt_websub:topic a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Topic"@en;
    rdfs:comment "The topic a publisher publishes on (and that subscribers subscribe to changes\n from)."@en.
inrupt_websub:callback a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Callback"@en;
    rdfs:comment "Subscriber's callback URL where content distribution notifications should be\n delivered."@en.
inrupt_websub:mode a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Mode"@en;
    rdfs:comment "'subscribe' or 'unsubscribe', depending on the goal of the request."@en.
inrupt_websub:challenge a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Challenge"@en;
    rdfs:comment "A hub-generated, random string that MUST be echoed by the subscriber to verify\n the subscription."@en.
inrupt_websub:leaseSeconds a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Lease (seconds)"@en;
    rdfs:comment "Number of seconds for which the subscriber would like to have the subscription\n active, given as a positive decimal integer."@en.
inrupt_websub:secret a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Secret"@en;
    rdfs:comment "A subscriber-provided cryptographically random unique secret string that will\n be used to compute an HMAC digest for authorized content distribution."@en.
inrupt_websub:subscriptionDenialReason a rdf:Property;
    rdfs:isDefinedBy <https://inrupt.com/vocab/service/websub#>;
    rdfs:label "Reason"@en;
    rdfs:comment "The hub may include a reason for which a subscription has been denied."@en.
inrupt_websub:x a rdfs:Literal;
    skos:definition "x [{{0}}]"@en, "x [{{0}}]"@es.
