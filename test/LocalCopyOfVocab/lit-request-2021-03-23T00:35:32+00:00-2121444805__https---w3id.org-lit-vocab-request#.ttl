@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix vann: <http://purl.org/vocab/vann/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix schema: <http://schema.org/>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix time: <http://www.w3.org/2006/time#>.
@prefix lit-request: <https://w3id.org/lit/vocab/request#>.

<https://w3id.org/lit/vocab/request#> a owl:Ontology;
    owl:versionInfo "0.1";
    dcterms:title "Solid Request Vocabulary"@en;
    dcterms:description "The Solid vocabulary providing terms for a various\n Named Graphs that make up a request. For example; Headers, Body, Metadata,\n ACL, etc."@en;
    dcterms:creator <https://inrupt.com/profile/card/#us>;
    dcterms:issued "2020/05/15"^^xsd:date;
    vann:preferredNamespacePrefix "lit-request";
    vann:preferredNamespaceUri "https://w3id.org/lit/vocab/request#";
    dcterms:license "http://www.opendatacommons.org/licenses/pddl/".
lit-request:header a rdf:Property;
    rdfs:label "Header"@en;
    rdfs:comment "The IRI of the Named Graph of a request containing header\n information (as triples)."@en.
lit-request:body a rdf:Property;
    rdfs:label "Body"@en;
    rdfs:comment "The IRI of the Named Graph of a request containing body\n information (as triples)."@en.
lit-request:acl a rdf:Property;
    rdfs:label "The IRI of the Named Graph of a request containing ACL\n information (as triples)."@en.
