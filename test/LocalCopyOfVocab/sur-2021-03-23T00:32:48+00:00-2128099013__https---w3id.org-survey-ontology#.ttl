@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix vann: <http://purl.org/vocab/vann/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix schema: <http://schema.org/>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix time: <http://www.w3.org/2006/time#>.
@prefix sur: <https://w3id.org/survey-ontology#>.

<https://w3id.org/survey-ontology#> a owl:Ontology, owl:NamedIndividual;
    owl:imports <http://purl.org/linked-data/cube#>, <http://purl.org/wf4ever/ro#>, <http://purl.org/wf4ever/wfdesc#>, <http://purl.org/wf4ever/wfprov#>, <http://w3id.org/rv-ontology#>, <http://www.w3.org/ns/prov#>;
    <http://purl.org/dc/elements/1.1/contributor> "Work on the Survey Ontology and Coney (https://survey.actionproject.eu/coney/) was partially supported by the ACTION project (grant agreement number 824603), co-funded by the European Commission under the Horizon 2020 Framework Programme."@en;
    <http://purl.org/dc/elements/1.1/creator> "Damiano Scandolari"@en, "Gloria Re Calegari"@en, "Irene Celino"@en, "Mario Scrocca"@en;
    <http://purl.org/dc/elements/1.1/rights> "The Survey Ontology is available under the Creative Commons Attribution 3.0 Unported license; see http://creativecommons.org/licenses/by/3.0/. In a nutshell, you are free to copy, distribute and transmit the work; to remix/adapt the work (e.g. to import the ontology and create specializations of its elements), as long as you attribute the work in the manner specified by the author or licensor (but not in any way that suggests that they endorse you or your use of the work). Proper Attribution: Simply include the statement \"This work is based on the Survey Ontology, developed by the Knowledge Technologies group at Cefriel, Milan\" and link back to https://w3id.org/survey-ontology"@en;
    <http://purl.org/dc/elements/1.1/subject> "Survey, Question, Answer, Variable"@en;
    <http://purl.org/dc/elements/1.1/title> "The Survey Ontology"@en;
    dcterms:license <http://creativecommons.org/licenses/by/3.0/>;
    vann:preferredNamespacePrefix "sur";
    vann:preferredNamespaceUri "https://w3id.org/survey-ontology#";
    rdfs:comment "Ontology for surveys based on the Coney data model."@en;
    rdfs:label "Survey Ontology"@en;
    owl:versionInfo "V 1.0, Release 2020"@en;
    <http://xmlns.com/foaf/0.1/homepage> <https://w3id.org/survey-ontology>.
<http://purl.org/dc/elements/1.1/contributor> a owl:AnnotationProperty.
<http://purl.org/dc/elements/1.1/creator> a owl:AnnotationProperty.
<http://purl.org/dc/elements/1.1/rights> a owl:AnnotationProperty.
<http://purl.org/dc/elements/1.1/subject> a owl:AnnotationProperty.
<http://purl.org/dc/elements/1.1/title> a owl:AnnotationProperty.
dcterms:license a owl:AnnotationProperty.
vann:preferredNamespacePrefix a owl:AnnotationProperty.
vann:preferredNamespaceUri a owl:AnnotationProperty.
<http://xmlns.com/foaf/0.1/homepage> a owl:AnnotationProperty.
sur:answeredIn a owl:ObjectProperty;
    rdfs:label "answered in";
    rdfs:domain sur:CompletedQuestion;
    rdfs:range sur:SurveyCompletionTask.
sur:CompletedQuestion a owl:Class;
    rdfs:label "Completed Question";
    rdfs:subClassOf <http://purl.org/linked-data/cube#Observation>, <http://purl.org/wf4ever/wfdesc#Output>, <http://www.w3.org/ns/prov#Entity>.
sur:SurveyCompletionTask a owl:Class;
    rdfs:label "Survey Completion Task";
    rdfs:subClassOf <http://purl.org/wf4ever/wfprov#ProcessRun>.
sur:completedFor a owl:ObjectProperty;
    rdfs:label "completed for";
    rdfs:domain sur:SurveyCompletionTask;
    rdfs:range sur:SurveyTarget.
sur:SurveyTarget a owl:Class;
    rdfs:label "Survey Target";
    rdfs:subClassOf owl:Thing.
sur:completesQuestion a owl:ObjectProperty;
    rdfs:label "completes question";
    rdfs:domain sur:CompletedQuestion;
    rdfs:range sur:Question.
sur:Question a owl:Class;
    rdfs:label "Question";
    rdfs:subClassOf sur:SurveyElement;
    owl:disjointWith sur:Talk.
sur:hasAnswer a owl:ObjectProperty;
    rdfs:label "has answer";
    rdfs:domain sur:CompletedQuestion;
    rdfs:range sur:Answer.
sur:Answer a owl:Class;
    rdfs:label "Answer";
    rdfs:subClassOf sur:SurveyElement;
    owl:disjointWith sur:Question, sur:Talk.
sur:hasLatentVariable a owl:ObjectProperty;
    rdfs:label "has latent variable";
    rdfs:domain sur:Question;
    rdfs:range sur:LatentVariable.
sur:LatentVariable a owl:Class;
    rdfs:label "Latent Variable";
    rdfs:subClassOf <http://w3id.org/rv-ontology#Variable>.
sur:hasObservableVariable a owl:ObjectProperty;
    rdfs:label "has observable variable";
    rdfs:domain sur:Question;
    rdfs:range sur:ObservableVariable.
sur:ObservableVariable a owl:Class;
    rdfs:label "Observable Variable";
    rdfs:subClassOf <http://w3id.org/rv-ontology#Variable>.
sur:hasVisualization a owl:ObjectProperty;
    rdfs:label "has visualization";
    rdfs:domain sur:MultipleChoiceQuestion;
    rdfs:range sur:QuestionVisualization.
sur:MultipleChoiceQuestion a owl:Class;
    rdfs:label "Multiple Choice Question";
    rdfs:subClassOf sur:ClosedQuestion.
sur:QuestionVisualization a owl:Class;
    rdfs:label "Question Visualization";
    rdfs:subClassOf owl:Thing.
sur:inSurveyProcedure a owl:ObjectProperty;
    rdfs:label "in survey procedure";
    rdfs:domain sur:SurveyElement;
    rdfs:range sur:SurveyProcedure.
sur:SurveyElement a owl:Class;
    rdfs:label "Survey Element";
    rdfs:subClassOf owl:Thing.
sur:SurveyProcedure a owl:Class;
    rdfs:label "Survey Procedure";
    rdfs:subClassOf <http://purl.org/wf4ever/ro#Resource>, <http://purl.org/wf4ever/wfdesc#Process>.
sur:leadsTo a owl:ObjectProperty;
    rdfs:label "leads to";
    rdfs:domain sur:SurveyElement;
    rdfs:range sur:SurveyElement.
sur:startsWith a owl:ObjectProperty;
    rdfs:label "starts with";
    rdfs:domain sur:SurveyProcedure;
    rdfs:range sur:SurveyElement.
sur:hasAnswerText a owl:DatatypeProperty;
    rdfs:label "has answer text";
    rdfs:domain sur:CompletedQuestion;
    rdfs:range xsd:string.
sur:hasCompletionTimestamp a owl:DatatypeProperty;
    rdfs:label "has completion timestamp";
    rdfs:domain sur:CompletedQuestion;
    rdfs:range xsd:dateTime.
sur:hasLink a owl:DatatypeProperty;
    rdfs:label "has link";
    rdfs:domain sur:Talk;
    rdfs:range xsd:anyURI.
sur:Talk a owl:Class;
    rdfs:label "Talk";
    rdfs:subClassOf sur:SurveyElement.
sur:hasOrderNumber a owl:DatatypeProperty;
    rdfs:label "has order number";
    rdfs:domain sur:ClosedAnswer;
    rdfs:range xsd:integer.
sur:ClosedAnswer a owl:Class;
    rdfs:label "Closed Answer";
    rdfs:subClassOf sur:Answer.
sur:hasPoints a owl:DatatypeProperty;
    rdfs:label "has points";
    rdfs:domain sur:ClosedAnswer;
    rdfs:range xsd:float.
sur:hasStatus a owl:DatatypeProperty;
    rdfs:label "has status";
    rdfs:domain sur:SurveyProcedure;
    rdfs:range xsd:string.
sur:hasText a owl:DatatypeProperty;
    rdfs:label "has text";
    rdfs:domain sur:SurveyElement;
    rdfs:range xsd:string.
sur:hasTitle a owl:DatatypeProperty;
    rdfs:label "has title";
    rdfs:domain sur:SurveyProcedure;
    rdfs:range xsd:string.
sur:hasValue a owl:DatatypeProperty;
    rdfs:label "has value";
    rdfs:domain sur:ClosedAnswer;
    rdfs:range xsd:float.
sur:participantId a owl:DatatypeProperty;
    rdfs:label "participant id";
    rdfs:domain sur:Participant;
    rdfs:range xsd:string.
sur:Participant a owl:Class;
    rdfs:label "Participant";
    rdfs:subClassOf <http://www.w3.org/ns/prov#Agent>.
sur:sessionId a owl:DatatypeProperty;
    rdfs:label "session id";
    rdfs:domain sur:SurveyCompletionTask;
    rdfs:range xsd:string.
<http://w3id.org/rv-ontology#Model> a owl:Class;
    rdfs:subClassOf <http://purl.org/wf4ever/ro#Resource>.
<http://w3id.org/rv-ontology#Variable> a owl:Class.
sur:CheckboxQuestion a owl:Class;
    rdfs:label "Checkbox Question";
    rdfs:subClassOf sur:ClosedQuestion.
sur:ClosedQuestion a owl:Class;
    rdfs:label "Closed Question";
    rdfs:subClassOf sur:Question.
sur:CompletedSurvey a owl:Class;
    rdfs:label "Completed Survey";
    rdfs:subClassOf <http://purl.org/linked-data/cube#Slice>, <http://purl.org/wf4ever/wfprov#Artifact>.
sur:MultipleInputQuestion a owl:Class;
    rdfs:label "Multiple Input Question";
    rdfs:subClassOf sur:OpenQuestion.
sur:OpenQuestion a owl:Class;
    rdfs:label "Open Question";
    rdfs:subClassOf sur:Question.
sur:OpenAnswer a owl:Class;
    rdfs:label "Open Answer";
    rdfs:subClassOf sur:Answer.
sur:SingleInputQuestion a owl:Class;
    rdfs:label "Single Input Question";
    rdfs:subClassOf sur:OpenQuestion.
sur:Survey a owl:Class;
    rdfs:label "Survey";
    rdfs:subClassOf <http://purl.org/wf4ever/ro#ResearchObject>.
sur:SurveyDataSet a owl:Class;
    rdfs:label "Survey Dataset";
    rdfs:subClassOf <http://purl.org/wf4ever/ro#Resource>, <http://purl.org/linked-data/cube#DataSet>.
<https://w3id.org/survey-ontology> a owl:NamedIndividual.
sur:Dropdown a sur:QuestionVisualization, owl:NamedIndividual;
    rdfs:label "Dropdown".
sur:Emoji a sur:QuestionVisualization, owl:NamedIndividual;
    rdfs:label "Emoji".
sur:Option a sur:QuestionVisualization, owl:NamedIndividual;
    rdfs:label "Option".
sur:Scale a sur:QuestionVisualization, owl:NamedIndividual;
    rdfs:label "Scale".
sur:Slider a sur:QuestionVisualization, owl:NamedIndividual;
    rdfs:label "Slider".
