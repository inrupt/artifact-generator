@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .

@prefix lit_core: <https://w3id.org/lit/vocab/core#> .
prefix test_vocab: <https://inrupt.com/vocab/static-test#>


test_vocab:
	a owl:Ontology, lit_core:Ontology ;
	owl:versionInfo "1" ;
	dcterms:creator "PatMcB" ;
	dcterms:date "2019/01/01" ;
 	dcterms:title "LIT Demo Vocab" ;
	dcterms:description "Testing static file won't cause re-generation." ;
	dcterms:license "http://purl.org/NET/rdflicense/MIT1.0.ttl" ;
	vann:preferredNamespacePrefix "test_vocab" ;
	vann:preferredNamespaceUri "https://inrupt.com/vocab/static-test#" .

test_vocab:sampleTerm a rdfs:Class ;
  rdfs:label "We have a label, but no comment!" .
